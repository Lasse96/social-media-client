{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["cypress"],
  "extends": ["plugin:cypress/recommended"],
  "overrides": [{ "files": ["**/*.js"] }],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": { "no-unused-vars": "off" }
}
